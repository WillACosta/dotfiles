{
  "sync.gist": "85919aaf36b3fed76177eaf8e97860a6",
  "editor.fontFamily": "JetBrains Mono, Zed Mono",
  "window.zoomLevel": 1,
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.8,
  "editor.fontSize": 14,
  "editor.tabSize": 2,
  "editor.renderLineHighlight": "gutter",
  "editor.cursorStyle": "line",
  "editor.rulers": [100, 120],
  "editor.autoClosingDelete": "always",
  "editor.suggest.preview": true,
  "editor.autoClosingQuotes": "always",
  "editor.autoClosingBrackets": "always",
  "editor.parameterHints.enabled": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports": "always",
    "source.addMissingImports": "always",
    "source.fixAll": "always"
  },
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.accessibilitySupport": "off",
  "editor.scrollbar.vertical": "auto",
  "editor.scrollbar.horizontal": "auto",
  "workbench.layoutControl.enabled": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.selectionHighlight": false,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "onlySnippets",
  "editor.wordBasedSuggestions": "off",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "explorer.sortOrder": "foldersNestsFiles",
  "editor.semanticHighlighting.enabled": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionWidget.includeNearbyQuickFixes": true,
  "editor.wordWrap": "on",
  "terminal.integrated.gpuAcceleration": "off",
  "terminal.integrated.middleClickBehavior": "paste",

  /// GIT Features
  "git.decorations.enabled": true,
  "git.autofetch": true,
  "git.blame.statusBarItem.enabled": true,
  "git.blame.editorDecoration.enabled": true,

  // Copilot
  "github.copilot.nextEditSuggestions.enabled": true,
  "editor.inlineSuggest.edits.renderSideBySide": "auto",

  "debug.openDebug": "openOnDebugBreak",
  "debug.internalConsoleOptions": "openOnSessionStart",
  "extensions.autoUpdate": true,
  "update.mode": "start",
  "javascript.autoClosingTags": true,
  "typescript.autoClosingTags": true,
  "html.autoClosingTags": true,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "debug.toolBarLocation": "commandCenter",
  "workbench.editor.showTabs": "multiple",

  /// editor UI settings begin

  "editor.minimap.enabled": false,
  "workbench.sideBar.location": "left",
  "workbench.editor.wrapTabs": true,

  /// editor UI settings end

  "workbench.editor.enablePreview": true,
  "mergeEditor.diffAlgorithm": "advanced",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "window.titleBarStyle": "custom",
  "window.nativeTabs": false,
  "window.commandCenter": true,
  "workbench.editor.showIcons": true,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.editor.decorations.colors": true,
  "workbench.editor.decorations.badges": true,
  "workbench.editor.labelFormat": "short",
  "workbench.startupEditor": "none",
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.lineHeight": 1.2,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "extensions.ignoreRecommendations": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.enableSmartCommit": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.config.dart": true,
    "**/.DS_Store": true,
    "**/.g.dart": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true
  },
  "files.associations": {
    ".sequelizerc": "javascript",
    ".stylelintrc": "json",
    ".prettierrc": "json",
    "*.tsx": "typescriptreact",
    ".env.*": "dotenv",
    ".env*": "dotenv",
    "*.css": "tailwindcss"
  },

  /// Python
  "python.languageServer": "Pylance",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },

  /// Dart and Flutter
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "dart.debugSdkLibraries": true,
  "dart.debugExternalPackageLibraries": true,
  "dart.flutterHotReloadOnSave": "all",
  "dart.showInspectorNotificationsForWidgetErrors": false,
  // "dart.flutterSdkPath": "/Users/will/fvm/default/bin",
  "[dart]": {
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off",
    "editor.formatOnSave": true,
    "editor.formatOnType": true
  },
  "dart.warnWhenEditingFilesOutsideWorkspace": false,
  "flutter-coverage.sufficientCoverageThreshold": 80,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.inlineSuggest.enabled": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  /// file nesting
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": ".eslint*, prettier*, tsconfig*, vite*, pnpm-lock*, bun.lockb, nest*, package-lock.json, yarn.lock",
    "tailwind.config.js": "tailwind.config*, postcss.config*",
    ".env.local": ".env*",
    ".env": ".env*",
    "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata",
    "*.dart": "${capture}.g.dart, ${capture}.chopper.dart",
    ".prettierrc": ".prettierignore",
    "jest.config.*": "jest.setup.*"
  },

  "cSpell.userWords": [
    "assinaturaremota",
    "Callout",
    "claude",
    "Cnpj",
    "collapsable",
    "devkit",
    "Excalidraw",
    "firestore",
    "genai",
    "Goldens",
    "graphcms",
    "langchain",
    "LTRB",
    "mobx",
    "nextjs",
    "originalname",
    "portalordem",
    "Qdrant",
    "signin",
    "situacao",
    "stackspot",
    "subscritions",
    "testid",
    "usecase",
    "Uuid",
    "vectorstore",
    "vectorstores",
    "venv",
    "vertexai",
    "voxel",
    "Whitespaces",
    "william",
    "wordpress",
    "xxxs",
    "ZOTP"
  ],
  "cSpell.language": "en, pt_BR",
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    ".fvm",
    "/fvm"
  ],
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.openRepositoryInParentFolders": "never",
  "tailwindCSS.experimental.classRegex": [
    ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
  ],
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  "http.proxySupport": "on",
  "http.proxyStrictSSL": false,
  "symbols.hidesExplorerArrows": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "tabnine.experimentalAutoImports": true,
  "workbench.iconTheme": "symbols",

  // prettier
  "prettier.semi": false,
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "prettier.printWidth": 80,
  "prettier.useTabs": false,
  "prettier.trailingComma": "all",
  "prettier.arrowParens": "always",
  // end prettier

  // APC settings
  "apc.font.family": "Inter",
  "workbench.colorCustomizations": {
    "editorCursor.foreground": "#c926c9",
    "terminalCursor.foreground": "#c926c9",
    "terminal.background": "#00000000"
  },
  "apc.electron": {
    "titleBarStyle": "hiddenInset",
    "trafficLightPosition": {
      "x": 11,
      "y": 10
    },
    "frame": false,
    "transparent": true,
    "vibrancy": "ultra-dark",
    "opacity": 0.98,
    "visualEffectState": "active",
    "backgroundColor": "rgba(123, 123, 123, 0.85)"
  },
  "apc.header": {
    "height": 36
  },
  "apc.listRow": {
    "height": 24
  },
  "apc.activityBar": {
    // "position": "top",
    "hideSettings": true,
    "size": 30,
    "itemMargin": 8,
    "itemSize": 32
  },
  "apc.stylesheet": {
    ".title-label > h2": "display: none",
    ".editor-actions": "display: none",
    ".pane-header": "padding: 0 8px",
    ".pane-body": "padding: 8px",
    ".nosidebar .inline-tabs-placeholder": "width: 75px",
    ".monaco-list-row": "border-radius: 4px;",
    ".split-view-view:first-child .pane-header": "display: none !important;",
    ".monaco-workbench .monaco-list:not(.element-focused):focus:before": "display: none;"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cloudcode.duetAI.project": "packers-342910",
  "security.promptForLocalFileProtocolHandling": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vscode_vibrancy.theme": "Default Dark",
  "diffEditor.codeLens": true,
  "workbench.colorTheme": "Mint Super Dark",
  "editor.tokenColorCustomizations": {
    "[*Light*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#000"
          }
        }
      ]
    },
    "[*Dark*]": {
      "textMateRules": [
        {
          "scope": "ref.matchtext",
          "settings": {
            "foreground": "#fff"
          }
        }
      ]
    },
    "textMateRules": []
  },
  "dotenv.enableAutocloaking": false,
  "explorer.confirmPasteNative": false,
  "workbench.settings.applyToAllProfiles": ["workbench.colorCustomizations"],
  "cloudcode.duetAI.inlineSuggestions.enableAuto": true,
  "workbench.preferredDarkColorTheme": "Bearded Theme Black & Emerald",
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "redhat.telemetry.enabled": true,
  "vscode-note.notespath": "~/workspace/notes",
  "window.customTitleBarVisibility": "auto"
}
